<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	  <constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <package name="org.tjise.util" extends="struts-default">
    	
        <action name="register" class="org.tjise.util.ValidateAciton" method="execute">
        	<result name="success">/success.jsp</result>
        	<result name="input">/register.jsp</result>
        </action>
        
        <action name="register2" class="org.tjise.util.ValidateAciton" method="execute2">
        	<result name="success">/success2.jsp</result>
        	<result name="input">/register.jsp</result>
        </action>
        
        <action name="register3" class="org.tjise.util.ValidateAciton" method="execute3">
        	<result name="success">/success2.jsp</result>
        	<result name="input">/success2.jsp</result>
        </action>
        
        <action name="register4" class="org.tjise.util.ValidateAciton" method="execute4">
        	<result name="success">/success2.jsp</result>
        	<result name="input">/success2.jsp</result>
        </action>
        
        <action name="quit" class="org.tjise.util.ValidateAciton" method="quit">
        	
        	<result name="input">/register.jsp</result>
        </action>
        
    </package>  
</struts>    
 
